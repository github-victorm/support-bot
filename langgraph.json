{
  "node_version": "20",
  "graphs": {
    "music_store": "./music_store_agent.py:create_graph"
  },
  "env": "./.env",
  "python_version": "3.11",
  "dependencies": ["."],
  "dockerfile_lines": [
    "COPY . /app",
    "WORKDIR /app",
    "RUN apt-get update && apt-get install -y python3-pip libsqlite3-dev",
    "RUN pip install --no-cache-dir --upgrade pip",
    "RUN pip install --no-cache-dir -r requirements.txt",
    "VOLUME [\"/app/music_store_vectors\"]"
  ]
}
